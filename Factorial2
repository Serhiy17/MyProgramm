import java.util.Scanner;

pablic class Factorial1{

	public static void main(String[] args){

		Scanner input = new Scanner(System.in);

		int x;
		int factoria2 = 1;
		int res = 1;
		int y;

		System.out.print("Please enter the y: ");
		y = input.nextInt();

		while (y > 0){

			while (y > 0){

			factorial = factorial*y;

			--y;
		}

		res = res + (x/factorial)

		--y;

		}

		System.out.print("Res is " + res);
	}
}